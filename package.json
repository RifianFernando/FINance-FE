        "eslint-config-next": "13.2.3",
        "next": "13.2.3",
        "react": "18.2.0",
        "react-dom": "18.2.0"
    },
    "devDependencies": {
        "@types/node": "^18.14.6",
        "@types/react": "^18.0.28",
        "eslint": "^8.35.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-prettier": "^8.7.0",
        "husky": "^8.0.0",
        "prettier": "^2.8.4",
        "typescript": "^4.9.5"
    }
}
